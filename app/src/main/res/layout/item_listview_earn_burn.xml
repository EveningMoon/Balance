<?xml version="1.0" encoding="utf-8" ?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp">

    <com.zhan.library.CircularView
        android:id="@+id/genericIcon"
        style="@style/CircularViewStyle_unselected"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/earnBurnName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="true"
        android:textSize="20sp"
        android:textColor="@color/harbor_rat"
        android:textStyle="bold"
        android:text="earn burn"
        android:layout_toRightOf="@+id/genericIcon"
        android:layout_marginLeft="20dp"
        android:layout_centerVertical="true" />

</RelativeLayout>
